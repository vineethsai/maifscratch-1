# LangGraph Multi-Agent RAG Configuration

# Gemini API (Required for fact-checking)
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: OpenAI API (for alternative LLM)
# OPENAI_API_KEY=your_openai_api_key_here

# Optional: Anthropic Claude API
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Vector Database Configuration
CHROMA_PERSIST_DIR=examples/langgraph/data/chroma_db
CHROMA_COLLECTION_NAME=maif_kb

# LangGraph Checkpointing
CHECKPOINT_DB=examples/langgraph/data/checkpoints_enhanced.db

# Session Management
SESSION_ARTIFACT_DIR=examples/langgraph/data/sessions

# Embedding Model (sentence-transformers)
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Search Configuration
TOP_K_RESULTS=5
SIMILARITY_THRESHOLD=0.7

# Logging
LOG_LEVEL=INFO
MAIF_LOG_LEVEL=DEBUG
